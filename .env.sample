NODE_ENV=development

# Backend server port
BACKEND_EXPOSE=10141
BACKEND_PORT=10141

# Frontend development server port  
FRONTEND_EXPOSE=10142

# Client URL (for CORS and OAuth redirects)
CLIENT_URL=http://localhost:10142

# MongoDB connection
MONGODB_URI=mongodb://localhost:10147/database
MONGODB_EXPOSE=10147
MONGODB_PORT=10147
MONGODB_DATABASE=database

# JWT secret key (generate a strong random string)
JWT_SECRET=jwt
# JWT expiration time
JWT_EXPIRES_IN=7d
# Session secret
SESSION_SECRET=secret

# Encryption key for API keys (at least 32 characters hex)
ENCRYPTION_KEY=key

# GitHub OAuth (https://github.com/settings/developers)
GITHUB_CLIENT_ID=id
GITHUB_CLIENT_SECRET=secret

# Google OAuth (https://console.developers.google.com/)
GOOGLE_CLIENT_ID=id
GOOGLE_CLIENT_SECRET=secret

# OpenAI API (https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=2000

# Judge0 API (https://rapidapi.com/judge0-official/api/judge0-ce)
REACT_APP_RAPID_API_URL=url
REACT_APP_RAPID_API_HOST=host
REACT_APP_RAPID_API_KEY=key

MAX_FILE_SIZE=10MB
UPLOAD_PATH=./server/uploads
ALLOWED_FILE_TYPES=.js,.ts,.py,.html,.css,.json,.md,.txt,.vue,.jsx,.tsx

RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=15
# Code execution rate limits
CODE_EXEC_LIMIT_PER_USER=10
CODE_EXEC_LIMIT_WINDOW=15