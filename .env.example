NODE_ENV=development
APP_NAME="GPT Collaboration App"
APP_VERSION=1.0.0

# Backend server port
BACKEND_EXPOSE=10141

# Frontend development server port  
FRONTEND_EXPOSE=10142

# Client URL (for CORS and OAuth redirects)
CLIENT_URL=http://localhost:10142

# MongoDB connection
MONGODB_URI=mongodb://localhost:10147/gpt-collab-dev
MONGO_EXPOSE=10147
MONGO_PORT=10147
MONGO_USER=admin
MONGO_PASSWORD=your-secure-mongo-password-here
MONGO_DATABASE=gpt-collab-dev

# JWT secret key (generate a strong random string)
JWT_SECRET=your-very-secure-jwt-secret-key-minimum-32-characters-long
# JWT expiration time
JWT_EXPIRES_IN=7d
# Session secret
SESSION_SECRET=your-secure-session-secret-here

# Encryption key for API keys (at least 32 characters hex)
ENCRYPTION_KEY=123-abc

# GitHub OAuth (https://github.com/settings/developers)
GITHUB_CLIENT_ID=123-abc
GITHUB_CLIENT_SECRET=123-abc

# Google OAuth (https://console.developers.google.com/)
GOOGLE_CLIENT_ID=123-abc
GOOGLE_CLIENT_SECRET=123-abc

# OpenAI API (https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4
OPENAI_MAX_TOKENS=2000

# Judge0 API (https://rapidapi.com/judge0-official/api/judge0-ce)
JUDGE0_API_KEY=0e361d0c28msha1d3c417fe67447p1c657bjsn83f44cf40bb9
JUDGE0_API_HOST=judge0-ce.p.rapidapi.com
JUDGE0_SUBMISSIONS_URL=https://judge0-ce.p.rapidapi.com/JUDGE0_SUBMISSIONS_URL

MAX_FILE_SIZE=10MB
UPLOAD_PATH=./server/uploads
ALLOWED_FILE_TYPES=.js,.ts,.py,.html,.css,.json,.md,.txt,.vue,.jsx,.tsx

RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW=15
# Code execution rate limits
CODE_EXEC_LIMIT_PER_USER=10
CODE_EXEC_LIMIT_WINDOW=15